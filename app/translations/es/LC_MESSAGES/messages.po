msgstr "Revisión de Errores"

#: app/templates/core/quiz_results_success.html:14
msgid "Question"
msgstr "Pregunta"

#: app/templates/core/quiz_results_success.html:15
msgid "Your answer"
msgstr "Tu respuesta"

#: app/templates/core/quiz_results_success.html:16
msgid "Correct answer"
msgstr "Respuesta correcta"

#: app/templates/core/quiz_results_success.html:22
msgid "What to do now?"
msgstr "¿Qué hacer ahora?"

#: app/templates/core/quiz_results_success.html:24
